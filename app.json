{
  "expo": {
    "name": "MMA Finds",
    "slug": "mma-finds-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main-logo/logo.png",
    "scheme": "com.mmafinds.app",
    "owner": "orestklymko",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "bundleIdentifier": "com.mmafinds.app",
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default"
        ]
      },
      "appleTeamId": "87TA2D22ST",
      "infoPlist": {
        "CFBundleDisplayName": "MMA Finds",
        "CFBundleName": "MMA Finds",
        "NSUserActivityTypes": []
      },
      "associatedDomains": [
        "applinks:mmafinds.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main-logo/splash.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mmafinds.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "mmafinds.com",
              "pathPrefix": "/s/"
            }
          ],
          "category": ["BROWSABLE","DEFAULT"]
        }
      ]
    },
    "plugins": [
      "./app.plugin.js",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/main-logo/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleWebClientId": "712660973446-9t222hmquqadadee9a3d31dqrhka89s5.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "330888a6-9e8b-416d-988a-51794fb23e33"
      }
    },
    "androidNavigationBar": {
        "hidden": true
    }
  }
}
