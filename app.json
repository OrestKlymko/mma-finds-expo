{
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-haptics"
      ]
    },
    "name": "MMA Finds",
    "slug": "mma-finds-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main-logo/logo.png",
    "scheme": "com.mmafinds.app",
    "owner": "orestklymko",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "@config-plugins/react-native-branch",
        {
          "apiKey": "key_live_grh585cIvSocBiDvpSS40aizwoZP243"
        }
      ],
      "expo-secure-store",
      "expo-build-properties",
      "expo-font"
    ],
    "ios": {
      "bundleIdentifier": "com.mmafinds.app",
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default"
        ]
      },
      "config": {
        "branch": {
          "apiKey": "key_live_grh585cIvSocBiDvpSS40jaizwoZP243"
        }
      },
      "appleTeamId": "87TA2D22ST",
      "infoPlist": {
        "CFBundleDisplayName": "MMA Finds",
        "CFBundleName": "MMA Finds",
        "NSUserActivityTypes": [],
        "NSUserNotificationsUsageDescription": "We use notifications to keep you informed."
      },
      "associatedDomains": [
        "applinks:mmafinds.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main-logo/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mmafinds.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "mmafinds.com",
              "pathPrefix": "/s/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleWebClientId": "712660973446-9t222hmquqadadee9a3d31dqrhka89s5.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "330888a6-9e8b-416d-988a-51794fb23e33"
